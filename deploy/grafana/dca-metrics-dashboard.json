      {
        "id": 1,
        "title": "HTTP Request Rate (Server)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(dca_server_http_requests_total[5m])",
            "legendFormat": "{{method}} {{path}} ({{status}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "unit": "reqps"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "text": {}
        },
        "pluginVersion": "8.0.0",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HTTP Response Times (Server)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dca_server_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(dca_server_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
